
<h3>Search Result </h3>
<table>
    <thead>
        <tr>
            <th>Cellphone</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @results.each do |result| %>
        <tr>
            <td><%= result.cellphone %></td>
            <td><%= result.first_name %></td>
            <td><%= result.last_name %></td>
            <td><%= result.email %></td>

            <td><%= link_to 'Show', result %></td>
            <td><%= link_to 'Edit', edit_user_path(result) %></td>
            <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </tbody>
</table>
<br>




